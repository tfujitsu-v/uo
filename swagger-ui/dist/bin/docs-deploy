#!/bin/bash

help="
Usage: bin/docs-deploy

APIドキュメントをS3にアップロードする
"
. "$(git rev-parse --show-toplevel)"/lib/bash/print-help.bash

set -e

script_dir=$(cd $(dirname ${BASH_SOURCE:-$0}); pwd)
cd "$script_dir/.."

./bin/cp-index

#aws s3 sync . s3://valotp/mesh-nearest-station-cache/docs/ --delete --cache-control "max-age=0"
aws s3 sync . s3://mixway-docs/docs/                       --delete --cache-control "max-age=0"

echo '正常終了'
